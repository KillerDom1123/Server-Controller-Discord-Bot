{
  "compilerOptions": {
    /* Language and Environment */
    "target": "es2022",

    /* Modules */
    "module": "commonjs",
    // "typeRoots": ["./typings",],

    /* Interop Constraints */
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,

    /* Type Checking */
    "strict": true,

    /* Completeness */
    "skipLibCheck": false
  }
}
